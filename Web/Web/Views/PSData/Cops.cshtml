@model Web.Models.PublicServiceData.PSDCar
@{
    ViewBag.Title = "Cops Data";
}

<div class="m-5">
    <hr />
    <ul class="collection with-header ml-auto mr-auto detailCollect">
        <li class="collection-header"><h4>@Resources.Global.Cops_title</h4></li>
        <a class="nolink" href="#" onclick="displayAccessInfoPopup(@Newtonsoft.Json.JsonConvert.SerializeObject(Model.Owner))"><li class="collection-item"><div><strong>@Resources.Global.Ower :</strong> @Html.DisplayFor(model => model.Owner.Name)<i class="fas fa-arrow-alt-circle-right secondary-content"></i></div></li></a>
        <a class="nolink" href="#" onclick="displayAccessInfoPopup(@Newtonsoft.Json.JsonConvert.SerializeObject(Model.Brand))"><li class="collection-item"><div><strong>@Resources.Global.Brand :</strong> @Html.DisplayFor(model => model.Brand.Name)<i class="fas fa-arrow-alt-circle-right secondary-content"></i></div></li></a>
        <a class="nolink" href="#" onclick="displayAccessInfoPopup(@Newtonsoft.Json.JsonConvert.SerializeObject(Model.NumberPlate))"><li class="collection-item"><div><strong>@Resources.Global.PlateNumber :</strong> @Html.DisplayFor(model => model.NumberPlate.Name)<i class="fas fa-arrow-alt-circle-right secondary-content"></i></div></li></a>
    </ul>
</div>

@Html.Partial("AccessInfo")